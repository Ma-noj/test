package edu.clarivate.pojosetterinjection;

import org.springframework.stereotype.Component;

@Component
public class Car {

	int id;
	String brand;
	double price;

	public void setId(int id) {
		this.id = id;
	}

	public void setBrand(String brand) {
		this.brand = brand;
	}

	public void setPrice(double price) {
		this.price = price;
	}

}
